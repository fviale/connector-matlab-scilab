<?xml version="1.0" encoding="ISO-8859-15"?>
<MatSciWorkerConfiguration>
    <MachineGroup hostname=".*">  <!-- This is a regular expression representing the machines targetted by this configuration, attributes can either be "hostname" or "ip" -->
        <matlab>
            <version>8.5</version>     <!-- The version of matlab -->
            <home>C:\Program Files\MATLAB\R2015a</home>    <!-- Matlab installation directory -->
            <!-- Example windows path: <home>C:\Program Files\Matlab2010b</home> -->
            <bindir>bin</bindir> <!-- Matlab bin directory (should not be changed) -->
            <command>matlab.exe</command>  <!-- Matlab executable name -->
            <arch>64</arch> <!-- Architecture 32 or 64 -->
            <!-- Matlab executable name on windows: matlab.exe -->
        </matlab>
    </MachineGroup>
</MatSciWorkerConfiguration>